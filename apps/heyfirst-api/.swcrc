{
  "module": {
    "type": "commonjs"
  },
  "jsc": {
    "baseUrl": "src",
    "paths": {
      "@/modules/*": ["modules/*"],
      "@/services/*": ["services/*"]
    },
    "target": "es2019",
    "parser": {
      "syntax": "typescript"
    },
    "minify": {
      "mangle": true,
      "compress": true
    }
  },
  "minify": true,
  "exclude": [
    "src/services/prisma/prisma-client-js/*.*" // generated by prisma
  ],
  "env": {
    "targets": {
      "node": 16
    }
  }
}
